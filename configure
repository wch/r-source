  LIBS1="${arg}"
else
  LIBS1="${LIBS1}${separator}${arg}"
fi
      ;;
    *)
      separator=""
test -z "${separator}" && separator=" "
if test -z "${LIBS2}"; then
  LIBS2="${arg}"
else
  LIBS2="${LIBS2}${separator}${arg}"
fi
      ;;
  esac
