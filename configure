  ac_cv_path_XMKMF="$XMKMF" # Let the user override the test with a dos path.
  ;;
  *)
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="${PATH}:/usr/bin/X11:/usr/X11R6/bin:/usr/openwin/bin"
  for ac_dir in $ac_dummy; do 
