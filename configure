{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if libcurl is version 7 and >= 7.22.0" >&5
$as_echo_n "checking if libcurl is version 7 and >= 7.22.0... " >&6; }
if ${r_cv_have_curl722+:} false; then :
  $as_echo_n "(cached) " >&6
