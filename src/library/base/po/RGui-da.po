msgid "Scrolling.\n"
msgstr "Rulle.\n"

#: src/gnuwin32/console.c:2302
msgid "  Keyboard: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n"
msgstr "  Tastatur: PgUp, PgDown, Ctrl+pile, Ctrl+Home, Ctrl+End,\n"

