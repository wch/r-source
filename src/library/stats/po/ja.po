#, c-format
msgid "'gradient' must be a numeric matrix of dimension (%d,%d)"
msgstr " 'gradient' は次元 (%d,%d) の数値行列でなければなりません"

#: port.c:506
#, c-format
msgid "fcn produced mode %d, length %d - wanted mode %d, length %d"
msgstr ""
" モード %d, 長さ %d の fcn が作られました - モード %d, 長さ %d が必要でした"

