#: fork.c:206 fork.c:209
msgid "unable to create a pipe"
msgstr "не могу создать трубу"

#: fork.c:226
#, c-format
msgid "unable to fork, possible reason: %s"
msgstr "не могу разветвить, возможная причина: %s"

#: fork.c:272
msgid "memory allocation error"
msgstr "ошибка распределения памяти"

#: fork.c:325
msgid "only children can send data to the master process"
msgstr "только дочерние процессы могут посылать данные мастер-процессу"

#: fork.c:327
msgid "there is no pipe to the master process"
msgstr "нет трубы к мастер-процессу"

#: fork.c:329
msgid "content to send must be RAW, use serialize() if needed"
msgstr "посылаемый контент должен быть RAW, используйте serialize(), если надо"

#: fork.c:338 fork.c:345 fork.c:669
msgid "write error, closing pipe to the master"
msgstr "ошибка записи, закрываю трубу к мастеру"

#: fork.c:358
msgid "only the master process can send data to a child process"
msgstr "только мастер-процесс может посылать данные дочернему процессу"

#: fork.c:365
#, c-format
msgid "child %d does not exist"
msgstr "дочерний процесс %d не существует"

#: fork.c:371
msgid "write error"
msgstr "ошибка записи"

#: fork.c:439 fork.c:561
#, c-format
msgid "error '%s' in select"
msgstr "ошибка '%s' в выборе"

#: fork.c:651
msgid "'mckill' failed"
msgstr "'mckill' не удался"

#: fork.c:661
msgid "'mcexit' can only be used in a child process"
msgstr "'mcexit' может быть использован только в дочернем процессе"

#: fork.c:682
msgid "'mcexit' failed"
msgstr "'mcexit' не удался"

#: fork.c:712 fork.c:721
msgid "invalid CPU affinity specification"
msgstr "неправильное описание привязки CPU"

#: fork.c:732
msgid "requested CPU set is too large for this system"
msgstr "запрошенная установка CPU слишком велика для этой системы"

#: fork.c:755
msgid "retrieving CPU affinity set failed"
msgstr "загрузка привязки установки CPU не удалась"
