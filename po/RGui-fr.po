#: console.c:1912
msgid "Scrolling.\n"
msgstr "Défilement.\n"

#: console.c:1913
msgid "  Keyboard: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n"
msgstr "  Clavier : PageHaut, PageBas, Ctrl+Flèches, Ctrl+Début, Ctrl+Fin,\n"

#: console.c:1914
msgid ""
"  Mouse: use the scrollbar(s).\n"
"\n"
